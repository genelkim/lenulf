(defun extract-noun-subcategory (triples flag)
;````````````````````````````````````````````
; Triples: a list of form (f count noun)
; Extract a sublist of form ((noun count) (noun count) ... (noun count))
;   where these elements had f = flag.
;
 (let ((trs triples) result)
      (loop 
         (when (eq (pop trs) flag)
               (push `(,(second trs) ,(car trs)) result)
               (setq trs (cddr trs)))
         (if (null trs) (return nil))
         (setq trs (cdddr trs))
         (if (null trs) (return nil)))
      (reverse result)
 )); end of extract-noun-subcategory
         

